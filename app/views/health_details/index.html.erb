<div class="page-header">
  <h1>
    Health details
    <a href="/health_details/new" class="btn btn-lg btn-success">New Health detail</a>
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>Actions</th>
      </tr>

      <% @health_details.each do |health_detail| %>
      <tr>
        <td>
          <a href="/health_details/<%= health_detail.id %>" class="btn btn-primary">Show</a>
          <a href="/health_details/<%= health_detail.id %>/edit" class="btn btn-warning">Edit</a>
          <a href="/delete_health_detail/<%= health_detail.id %>" class="btn btn-danger" rel="nofollow">Delete</a>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
